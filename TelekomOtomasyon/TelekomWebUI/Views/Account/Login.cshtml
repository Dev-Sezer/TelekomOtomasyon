@model TelekomWebUI.Models.ViewModels.LoginViewModel
<!DOCTYPE HTML>
<html>
<head>
	<title>Augment an Admin Panel Category Flat Bootstrap Responsive Web Template | Login :: w3layouts</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="keywords" content="Augment Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
	<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
	<!-- Bootstrap Core CSS -->
	<link href="~/theme/css/bootstrap.min.css" rel='stylesheet' type='text/css' />
	<!-- Custom CSS -->
	<link href="~/theme/css/style.css" rel='stylesheet' type='text/css' />
	<!-- Graph CSS -->
	<link href="~/theme/css/font-awesome.css" rel="stylesheet">
	<!-- jQuery -->
	<link href='//fonts.googleapis.com/css?family=Roboto:700,500,300,100italic,100,400' rel='stylesheet' type='text/css'>
	<!-- lined-icons -->
	<link rel="stylesheet" href="~/theme/css/icon-font.min.css" type='text/css' />
	<!-- //lined-icons -->
	<script src="~/theme/js/jquery-1.10.2.min.js"></script>
	<!--clock init-->
</head>
<body>
	<!--/login-->

	<div class="error_page">
		<!--/login-top-->

		<div class="error-top">
			<h2 class="inner-tittle page">Müşteri Giriş Paneli</h2>
			<div class="login">
				<h3 class="inner-tittle t-inner">Login</h3>
				<div class="buttons login">

				</div>
				<form asp-controller="Account" asp-action="Login" method="post">
					<input type="text" class="text" name="Name" value="İsim" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Name';}">
					<input type="text" value="Soyisim" name="Surname" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Surname';}">
					<input type="text" value="Tc" name="SerialNumber" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'SerialNumber';}">
					<div class="submit"><input type="submit" onclick="myFunction()" value="Login"></div>
					<div class="clearfix"></div>
				</form>
			</div>


		</div>


		<!--//login-top-->
	</div>

	<!--//login-->
	<!--footer section start-->
	<div class="footer">
		<div class="error-btn">
			<a asp-controller="Home" asp-action="Index" class="read fourth" >Return to Home</a>
		</div>
		<p>&copy 2016 Augment . All Rights Reserved | Design by <a href="https://w3layouts.com/" target="_blank">W3layouts.</a></p>
	</div>
	<!--footer section end-->
	<!--/404-->
	<!--js -->
	<script src="~/theme/js/jquery.nicescroll.js"></script>
	<script src="~/theme/js/scripts.js"></script>
	<!-- Bootstrap Core JavaScript -->
	<script src="~/theme/js/bootstrap.min.js"></script>
</body>
</html>
